{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [],
    "centerOnLaunch": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "initialCols": 120,
    "initialPosition": "0,0",
    "initialRows": 40,
    "keybindings": 
    [
        {
            "id": "Terminal.PreviousTab",
            "keys": "ctrl+shift+tab"
        },
        {
            "id": "Terminal.CopyToClipboard",
            "keys": "ctrl+c"
        },
        {
            "id": "Terminal.ClosePane",
            "keys": "ctrl+shift+w"
        },
        {
            "id": "Terminal.FindText",
            "keys": "ctrl+shift+f"
        },
        {
            "id": "Terminal.PasteFromClipboard",
            "keys": "ctrl+v"
        },
        {
            "id": "Terminal.DuplicatePaneAuto",
            "keys": "alt+shift+d"
        },
        {
            "id": "Terminal.SplitPaneVertical",
            "keys": "alt+\\"
        },
        {
            "id": "Terminal.NewTab",
            "keys": "ctrl+shift+t"
        },
        {
            "id": "Terminal.CloseTab",
            "keys": "ctrl+w"
        },
        {
            "id": "Terminal.SplitPaneHorizontal",
            "keys": "alt+minus"
        },
        {
            "id": "Terminal.MoveFocusLeft",
            "keys": "alt+h"
        },
        {
            "id": "Terminal.MoveFocusRight",
            "keys": "alt+l"
        },
        {
            "id": "Terminal.MoveFocusUp",
            "keys": "alt+k"
        },
        {
            "id": "Terminal.MoveFocusDown",
            "keys": "alt+j"
        },
        {
            "id": "Terminal.ResizePaneLeft",
            "keys": "alt+shift+h"
        },
        {
            "id": "Terminal.ResizePaneRight",
            "keys": "alt+shift+l"
        },
        {
            "id": "Terminal.ResizePaneUp",
            "keys": "alt+shift+k"
        },
        {
            "id": "Terminal.ResizePaneDown",
            "keys": "alt+shift+j"
        },
        {
            "id": "Terminal.NextTab",
            "keys": "ctrl+tab"
        },
        {
            "id": "Terminal.SwitchToTab0",
            "keys": "ctrl+1"
        },
        {
            "id": "Terminal.SwitchToTab1",
            "keys": "ctrl+2"
        },
        {
            "id": "Terminal.SwitchToTab2",
            "keys": "ctrl+3"
        },
        {
            "id": "Terminal.SwitchToTab3",
            "keys": "ctrl+4"
        },
        {
            "id": "Terminal.SwitchToTab4",
            "keys": "ctrl+5"
        },
        {
            "id": "Terminal.SwitchToTab5",
            "keys": "ctrl+6"
        },
        {
            "id": "Terminal.SwitchToTab6",
            "keys": "ctrl+7"
        },
        {
            "id": "Terminal.CommandPalette",
            "keys": "ctrl+shift+p"
        },
        {
            "id": "Terminal.SwitchToTab7",
            "keys": "ctrl+8"
        },
        {
            "id": "Terminal.SwitchToTab8",
            "keys": "ctrl+9"
        }
    ],
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": 
